<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.hungerstrike.model">
    <class name="Orders" table="orders">
        <id name="orderid" column="orderid">
        </id>
        <property name="restaurantid" column="restaurantid" />
        <property name="userid" column="userid" />
        <property name="agentid" column="agentid" />
        <property name="totalprice" column="totalprice" />
        <property name="totalpreptime" column="totalpreptime" />
        <property name="preference" column="preference" />
       
    </class> 
</hibernate-mapping>