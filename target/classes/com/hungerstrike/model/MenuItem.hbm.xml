<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.hungerstrike.model">
    <class name="MenuItem" table="MENU_ITEM">
        <id name="itemId" column="ITEM_ID">
            <generator class="native"/>
        </id>
        <property name="itemName" column="ITEM_NAME" />
        <property name="price" column="PRICE" />
        <property name="prepTime" column="PREP_TIME" />
        <property name="menuId" column="MENU_ID" />
    </class> 
</hibernate-mapping>